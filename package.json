{
	"name": "server",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "npm run build && npm run copy-knex && npm run watch",
		"serve": "nodemon dist/server.js",
		"watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
		"build": "npm run build-ts",
		"build-ts": "tsc",
		"watch-ts": "tsc -w",
		"copy-knex": "cp src/knexfile.js dist/knexfile.js && cp -aR src/config/ dist/config",
		"test": "jest"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/express": "^4.16.1",
		"@types/jest": "^24.0.11",
		"@types/jwt-simple": "^0.5.33",
		"@types/knex": "^0.15.2",
		"@types/lodash": "^4.14.195",
		"@types/moment": "^2.13.0",
		"@types/morgan": "^1.7.35",
		"@types/node": "^11.10.5",
		"@types/passport": "^1.0.0",
		"@types/passport-jwt": "^3.0.1",
		"@types/passport-local": "^1.0.33",
		"@types/ramda": "^0.26.3",
		"jest": "^24.5.0",
		"typescript": "^3.4.1"
	},
	"dependencies": {
		"@types/node": "^11.10.5",
		"babel-polyfill": "^6.26.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.18.3",
		"concurrently": "^4.1.0",
		"debug": "^4.1.1",
		"dotenv": "^7.0.0",
		"express": "^4.16.4",
		"jwt-simple": "^0.5.5",
		"knex": "^0.13.0",
		"lodash": "^4.17.21",
		"moment": "^2.24.0",
		"morgan": "~1.8.1",
		"mssql": "^3.3.0",
		"nodemon": "^1.18.10",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"path": "^0.12.7",
		"ramda": "^0.26.1",
		"typescript": "^3.4.1"
	}
}
